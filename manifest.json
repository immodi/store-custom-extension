{
    "manifest_version": 2,
    "name": "APA",
    "version": "1.0",
    "description": "",
    "applications": {
        "gecko": {
            "id": "sagl@example.com",
            "strict_min_version": "57.0"
        }
    },
    "permissions": ["activeTab", "storage"],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"],
        "type": "module"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}
